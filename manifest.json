{
  "name": "Pomodoro Timer",
  "description": "Adjustable Pomodoro timer to fit all your study needs",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}