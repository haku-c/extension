{
  "name": "Pomodoro Timer",
  "description": "Adjustable Pomodoro timer to fit all your study needs",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}